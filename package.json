{
  "name": "sidetrek-cli",
  "version": "0.1.42",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run index.ts",
    "dev": "CUSTOM_ENV=development bun build --compile --sourcemap ./index.ts --outfile ./build/sidetrek --watch && rm -rf ./.*.bun-build",
    "build": "bun build --compile --sourcemap ./index.ts --outfile ./build/sidetrek",
    "release": "bun run ./src/scripts/release.ts",
    "prod-release": "bun run ./src/scripts/release.ts --production"
  },
  "devDependencies": {
    "@types/bun": "^1.1.0",
    "@types/ramda": "^0.29.12",
    "@types/semver": "^7.5.8",
    "@types/uuid": "^9.0.8",
    "tar": "^7.0.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "bun": "^1.1.5",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "is-unicode-supported": "^2.0.0",
    "ky": "^1.3.0",
    "ramda": "^0.29.1",
    "semver": "^7.6.2",
    "uuid": "^9.0.1",
    "yaml": "^2.4.1"
  }
}
