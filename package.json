{
  "name": "sidetrek-cli",
  "version": "0.1.4",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run index.ts",
    "dev": "bun build ./index.ts --compile --sourcemap --outfile ./build/sidetrek --watch && rm -rf ./*.bun-build",
    "build": "bun build ./index.ts --compile --sourcemap --outfile ./build/sidetrek",
    "release": "bun run ./src/scripts/release.ts --production",
    "local-release": "bun run ./src/scripts/release.ts"
  },
  "devDependencies": {
    "@types/bun": "^1.1.0",
    "@types/ramda": "^0.29.12",
    "tar": "^7.0.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "bun": "^1.1.5",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "is-unicode-supported": "^2.0.0",
    "ramda": "^0.29.1",
    "yaml": "^2.4.1"
  }
}
